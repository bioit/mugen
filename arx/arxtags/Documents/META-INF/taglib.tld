        </attribute>   
        <attribute>
            <name>cancelName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>            
        </attribute>   
        <attribute>
            <name>onClick</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>            
        </attribute>                      
    </tag>          
    <tag>
        <name>back</name>
        <tagclass>com.arexis.arxtags.BackTag</tagclass>
        <info>Creates a back button</info>      
    </tag>
    <tag>
        <name>hide-collection</name>
        <tagclass>com.arexis.arxtags.HideCollectionTag</tagclass>
        <attribute>
            <name>collection</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>limit</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue> 
        </attribute>        
    </tag>
    <tag>
        <name>parameter</name>
        <tagclass>com.arexis.arxtags.ParameterTag</tagclass>
        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag> 
</taglib>
