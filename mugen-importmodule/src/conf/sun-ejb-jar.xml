<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 EJB 2.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_2_1-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>ImporterBean</ejb-name>
      <jndi-name>ejb/ImporterBean</jndi-name>
      <ejb-ref>
        <ejb-ref-name>ejb/ImportJobBean</ejb-ref-name>
        <jndi-name>ejb/ImportJobBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/SamplingUnitManagerBean</ejb-ref-name>
        <jndi-name>ejb/SamplingUnitManagerBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/GenotypeManagerBean</ejb-ref-name>
        <jndi-name>ejb/GenotypeManagerBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/ResourceManagerBean</ejb-ref-name>
        <jndi-name>ejb/ResourceManagerBean</jndi-name>
      </ejb-ref>
      <resource-ref>
        <res-ref-name>jms/ImpDestinationFactory</res-ref-name>
        <jndi-name>jms/ImpDestinationFactory</jndi-name>
      </resource-ref>
    </ejb>
    <ejb>
      <ejb-name>ImpBean</ejb-name>
      <jndi-name>jms/ImpBean</jndi-name>
      <ejb-ref>
        <ejb-ref-name>ejb/ImporterBean</ejb-ref-name>
        <jndi-name>ejb/ImporterBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/SamplingUnitManagerBean</ejb-ref-name>
        <jndi-name>ejb/SamplingUnitManagerBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/ModelManagerBean</ejb-ref-name>
        <jndi-name>ejb/ModelManagerBean</jndi-name>
      </ejb-ref>
      <mdb-connection-factory>
        <jndi-name>jms/ImpDestinationFactory</jndi-name>
      </mdb-connection-factory>
    </ejb>
    <ejb>
      <ejb-name>ImportJobBean</ejb-name>
      <jndi-name>ejb/ImportJobBean</jndi-name>
      <ejb-ref>
        <ejb-ref-name>ejb/UserBean</ejb-ref-name>
        <jndi-name>ejb/UserBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/FileBean</ejb-ref-name>
        <jndi-name>ejb/FileBean</jndi-name>
      </ejb-ref>
      <ejb-ref>
        <ejb-ref-name>ejb/ProjectBean</ejb-ref-name>
        <jndi-name>ejb/ProjectBean</jndi-name>
      </ejb-ref>
    </ejb>
    <message-destination>
      <message-destination-name>ImpDestination</message-destination-name>
      <jndi-name>jms/ImpBean</jndi-name>
    </message-destination>
  </enterprise-beans>
</sun-ejb-jar>
